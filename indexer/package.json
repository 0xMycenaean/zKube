{
  "name": "indexer",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "clean": "rm -rf lib",
    "build": "yarn clean && tsc",
    "db:migration:generate": "rm -rf db && npx squid-typeorm-migration generate",
    "db:migration:apply": "npx squid-typeorm-migration apply",
    "db:down": "docker compose down",
    "db:up": "docker compose up -d",
    "start": "node -r dotenv/config lib/main.js"
  },
  "dependencies": {
    "@subsquid/archive-registry": "^3.3.0",
    "@subsquid/evm-processor": "^1.16.0",
    "@subsquid/graphql-server": "^4.5.0",
    "@subsquid/typeorm-migration": "^1.3.0",
    "@subsquid/typeorm-store": "^1.2.6",
    "ethers": "^6.11.1",
    "typeorm": "^0.3.20"
  },
  "devDependencies": {
    "@subsquid/evm-typegen": "^3.3.0",
    "@subsquid/typeorm-codegen": "^1.3.3",
    "typescript": "^5.3.3"
  }
}
