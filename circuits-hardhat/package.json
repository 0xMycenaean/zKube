{
  "name": "circuits-hardhat",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "devDependencies": {
    "chai": "^4.3.6",
    "hardhat": "^2.9.1",
    "hardhat-circom": "^3.3.2",
    "ts-node": "^10.9.1",
    "typescript": "^5.3.2"
  },
  "engines": {
    "node": ">=16"
  },
  "scripts": {
    "circom:dev": "hardhat circom  --circuit main --deterministic --debug --verbose",
